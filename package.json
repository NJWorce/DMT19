{
  "name": "DMT19",
  "version": "1.0.0",
  "repository": "https://github.com/NJWorce/DMT19",
  "main": "app.js",
  "private": true,
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": [
          "app.js"
        ],
        "msg": "Ulta - Voice on Google Cloud Platform"
      }
    }
  },
  "scripts": {
    "start": "node app.js",
    "test": "repo-tools test app && ava -t 30s --tap test/*.test.js",
    "e2e": "repo-tools test deploy && E2E_TESTS=True ava -t 30s --tap test/*.test.js",
    "cover": "nyc --cache npm test; nyc report --reporter=html"
  },
  "dependencies": {
    "@google-cloud/datastore": "^4.0.0",
    "body-parser": "1.19.0",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "nconf": "^0.10.0",
    "prompt": "^1.0.0",
    "pug": "^2.0.3",
    "uglify-js": "^3.4.9"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.0.0",
    "ava": "^0.25.0",
    "proxyquire": "^2.1.0",
    "sinon": "^7.0.0"
  }
}